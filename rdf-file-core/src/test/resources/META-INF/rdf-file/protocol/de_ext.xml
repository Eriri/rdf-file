<protocol name="de_ext">
	<head>
		<row>
			<column>
				<output>${column.desc()}</output>
				<output>:</output>
				<output>${column.value()}</output>
			</column>
		</row>
	</head>
	<body>
		<row>
			<column>
				<output>${column.value()}</output>
			</column>
		</row>
	</body>
	<tail>
		<row>
			<column>
				<output>${column.value()}</output>
			</column>
		</row>
	</tail>
</protocol>
